<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itp341.caceres.nicholas.puzzleGame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="itp341.caceres.nicholas.puzzleGame.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="itp341.caceres.nicholas.puzzleGame.NamePuzzle" />
        <activity android:name="itp341.caceres.nicholas.puzzleGame.ColorPuzzle" />
        <activity android:name="itp341.caceres.nicholas.puzzleGame.SizePuzzle" />
        <activity android:name="itp341.caceres.nicholas.puzzleGame.SeasonsPuzzle" />
        <activity android:name="itp341.caceres.nicholas.puzzleGame.WinLoseActivity"></activity>
    </application>

</manifest>
